{
  "name": "redux-rails",
  "version": "1.0.0",
  "description": "Redux and your server talking without fuss.",
  "main": "dist/redux-rails.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/instacart/redux-rails"
  },
  "keywords": [
    "instacart",
    "redux",
    "rest",
    "rails",
    "redux-rails"
  ],
  "scripts": {
    "analyze-webpack-bundles": "analyze=true webpack --progress --config webpack.config.js",
    "start": "webpack --progress --watch --config webpack.config.js",
    "build": "npm test && webpack --progress --config webpack.config.js",
    "test": "jest --coverage --silent",
    "test:watch": "npm test -- --watch --bail"
  },
  "author": "Dominic Cocchiarella (http://github.com/dcocchia)",
  "license": "Apache-2.0",
  "peerDependencies": {
    "redux": ">=3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.4.2",
    "nock": "^9.0.2",
    "path": "^0.12.7",
    "redux": ">=3.6.0",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec.js)$",
    "collectCoverageFrom": [
      "src/**"
    ]
  }
}
